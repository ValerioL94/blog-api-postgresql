{
  "name": "blog-backend",
  "version": "1.0.0",
  "description": "API only backend site express-typescript-postgresql-prisma",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test && vitest run --pool=forks",
    "dev": "NODE_ENV=development && tsx --watch --env-file=.env app.ts",
    "build": "tsc",
    "start": "prisma db push && node app.js",
    "kill": "kill -9 $(lsof -t -i:3000)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ValerioL94/blog-api-postgresql.git"
  },
  "keywords": [
    "backend",
    "restful",
    "api",
    "express",
    "postgresql",
    "prisma"
  ],
  "author": "ValerioL94",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ValerioL94/blog-api-postgresql/issues"
  },
  "homepage": "https://github.com/ValerioL94/blog-api-postgresql#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "prisma": "^5.19.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.2.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.1"
  }
}
